# Git abbreviations
abbr -a -g g git
abbr -a -g gs git st
abbr -a -g gl git lg
abbr -a -g gla git lg --all
abbr -a -g glm 'git log --cherry-pick --oneline --left-right --graph HEAD...master'
abbr -a -g gci 'git ci -m'
abbr -a -g gca 'git ci --amend'
abbr -a -g gco git co
abbr -a -g gd git diff
abbr -a -g ga git add
